<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ru.nsu.template">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:name="ru.nsu.template.TemplateApplication"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="ru.nsu.template.presentation.breedlist.BreedListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ru.nsu.template.presentation.breed.BreedActivity"/>
        <activity android:name="ru.nsu.template.presentation.breed.subbreed.SubBreedActivity" />
    </application>

</manifest>